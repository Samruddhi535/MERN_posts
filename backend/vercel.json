{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/users",
        "methods": ["POST", "GET"],
        "dest": "server.js"
      },
      {
        "src": "/api/users/:id",
        "methods": ["DELETE", "GET", "PATCH"],
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  